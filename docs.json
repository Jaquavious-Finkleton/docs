{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Navae",
  "colors": {
    "primary": "#108080",
    "light": "#108080",
    "dark": "#108080"
  },
  "favicon": "/app-icon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "icon": "book-open-cover",
          "color": {
            "light": "#108080",
            "dark": "#108080"
          },
          "href": "https://navae.mintlify.com/docs"
        },
        {
          "anchor": "Website",
          "icon": "arrow-up-right",
          "color": {
            "light": "#108080",
            "dark": "#108080"
          },
          "href": "https://navae.framer.ai"
        }
      ]
    },
    "groups": [
      {
        "group": "Untitled Group",
        "pages": [
          "untitled-group/untitled-page"
        ]
      }
    ]
  },
  "logo": "/images/app-icon.svg",
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:navae.framer.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "fonts": {
    "family": "Comfortaa"
  },
  "appearance": {
    "default": "system"
  },
  "background": {
    "color": {
      "light": "#EEEEEE",
      "dark": "#101010"
    }
  },
  "styling": {
    "latex": true
  }
}
